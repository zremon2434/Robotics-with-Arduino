int onee=13;
int two=12;
int three=8;
int four=4;
int dah=2000;


void setup()
{
pinMode(onee,OUTPUT);
pinMode(two,OUTPUT);
pinMode(three,OUTPUT);
pinMode(four,OUTPUT);
  
  
  
  
}

void loop()
{

  digitalWrite(onee,LOW);
  
   digitalWrite(two,LOW);
  
   digitalWrite(three,LOW);
  
   digitalWrite(four,LOW);
  
  delay(dah);
  
  
  
  
  digitalWrite(onee,LOW);

   digitalWrite(two,LOW);
 
   digitalWrite(three,LOW);
  
   digitalWrite(four,HIGH);
  delay(dah);
  
  
  
  
  digitalWrite(onee,LOW);
  
   digitalWrite(two,LOW);
  
   digitalWrite(three,HIGH);
 
   digitalWrite(four,LOW);

  delay(dah);
  
  
  
  
  
  digitalWrite(onee,LOW);
 
   digitalWrite(two,LOW);
  
   digitalWrite(three,HIGH);
  
   digitalWrite(four,HIGH);
 
  delay(dah);
  
  
  
  
  
  digitalWrite(onee,LOW);

   digitalWrite(two,HIGH);
  
   digitalWrite(three,LOW);
 
   digitalWrite(four,LOW);
 
  delay(dah);
  
  
  
  
  
  digitalWrite(onee,LOW);
 
   digitalWrite(two,HIGH);
 
   digitalWrite(three,LOW);
 
   digitalWrite(four,HIGH);

  delay(dah);
  
  
  
  
  
  digitalWrite(onee,LOW);

   digitalWrite(two,HIGH);
  
   digitalWrite(three,HIGH);
  
   digitalWrite(four,LOW);
 
  delay(dah);
  
  
  
  
  
  digitalWrite(onee,LOW);

   digitalWrite(two,HIGH);

   digitalWrite(three,HIGH);

   digitalWrite(four,HIGH);
 
  delay(dah);
  
  
  
  
  
  digitalWrite(onee,HIGH);
 
   digitalWrite(two,LOW);

   digitalWrite(three,LOW);
  
   digitalWrite(four,LOW);

  delay(dah);
  
  
  
  
  
  digitalWrite(onee,HIGH);

   digitalWrite(two,LOW);
 
   digitalWrite(three,LOW);
 
   digitalWrite(four,HIGH);

  delay(dah);
  
  
  
  
  
  
  digitalWrite(onee,HIGH);

   digitalWrite(two,LOW);

   digitalWrite(three,HIGH);
 
   digitalWrite(four,LOW);
 
  delay(dah);
  
  
  
  
  
  
  digitalWrite(onee,HIGH);
 
   digitalWrite(two,LOW);
 
   digitalWrite(three,HIGH);
 
   digitalWrite(four,HIGH);
  
  delay(dah);
  
  
  
  
  
  digitalWrite(onee,HIGH);
 
   digitalWrite(two,HIGH);
  
   digitalWrite(three,LOW);

   digitalWrite(four,LOW);

  delay(dah);
  
  
  
  
  
  
  digitalWrite(onee,HIGH);
 
   digitalWrite(two,HIGH);

   digitalWrite(three,LOW);
 
   digitalWrite(four,HIGH);

  delay(dah);
  
  
  
  
  
  
  digitalWrite(onee,HIGH);

   digitalWrite(two,HIGH);
 
   digitalWrite(three,HIGH);
  
   digitalWrite(four,LOW);
 
  delay(dah);
  
  
  
  
  
  
  digitalWrite(onee,HIGH);
 
   digitalWrite(two,HIGH);

   digitalWrite(three,HIGH);

   digitalWrite(four,HIGH);
  
  delay(dah);
  
  
}          
  
